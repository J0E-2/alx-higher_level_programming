#!/usr/bin/python3
def safe_print_division(a, b):
    """
    function divides 2 integers and prints the result.
    
    Returns the value of the division, otherwise: None
    """
    try:
        c = a / b
        
    except ZeroDivisionError:
        c = None

    finally:
        print("Inside result: {}".format(c))
        return (c)
